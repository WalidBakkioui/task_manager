<IfModule mod_rewrite.c>
    RewriteEngine On

    # Acc√®s direct aux fichiers existants
    RewriteCond %{REQUEST_FILENAME} -f
    RewriteRule ^ - [L]

    # Redirige tout vers index.php
    RewriteRule ^ index.php [QSA,L]
</IfModule>

<IfModule !mod_rewrite.c>
    ErrorDocument 404 /index.php
</IfModule>
